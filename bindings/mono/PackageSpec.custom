
      public PackageSpec () {}

      [DllImport("libredcarpet")]
	  static extern int rc_package_spec_equal(IntPtr a, IntPtr b);

	  public override bool Equals (object other) {
	      int ret = rc_package_spec_equal (this.Handle,
                                               ((PackageSpec) other).Handle);
	      if (ret == 0)
                  return false;

	      return true;
	  }


      protected PackageSpec () {}

      [DllImport("libredcarpet")]
	  static extern int rc_package_spec_equal(RC.PackageSpec a, RC.PackageSpec b);

	  public bool Equals (RC.PackageSpec other) {
	    int ret = rc_package_spec_equal (this, other);
	    if (ret == 0)
		 return false;
	    return true;
	  }
